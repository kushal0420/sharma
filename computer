            double expectedResult = uservalue1 - uservalue2;

            Calc testCalc = new Calc(uservalue1, uservalue2);

            //Act 
            double actualResult = testCalc.GetSubtraction();

            //Assert 
            Assert.AreEqual(expectedResult, actualResult);
        }
